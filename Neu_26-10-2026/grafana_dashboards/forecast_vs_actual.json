{
  "title": "Phoenyra \u2013 Forecast vs Actual",
  "panels": [
    {
      "type": "timeseries",
      "title": "Price Forecast vs Actual",
      "targets": [
        {
          "expr": "pho_forecast_price_eur_mwh"
        },
        {
          "expr": "pho_mark"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Load Forecast vs Actual",
      "targets": [
        {
          "expr": "pho_forecast_load_mw"
        },
        {
          "expr": "pho_grid_load_mw"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Solar/Wind Forecast vs Actual",
      "targets": [
        {
          "expr": "pho_forecast_solar_mw"
        },
        {
          "expr": "pho_forecast_wind_mw"
        }
      ]
    }
  ]
}