{
  "title": "Phoenyra \u2013 Backoffice KPIs",
  "panels": [
    {
      "type": "stat",
      "title": "Confirm Lag (avg min)",
      "targets": [
        {
          "expr": "pho_bo_confirm_lag_min"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Mismatch Count (T+1)",
      "targets": [
        {
          "expr": "pho_bo_mismatch_count"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Settlements per day",
      "targets": [
        {
          "expr": "pho_bo_settlements_total"
        }
      ]
    }
  ]
}